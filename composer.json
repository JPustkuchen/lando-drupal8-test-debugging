{
  "name": "drupal/recommended-project",
  "description": "Project template for Drupal 8 projects with a relocated document root",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "homepage": "https://www.drupal.org/project/drupal",
  "support": {
    "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
    "chat": "https://www.drupal.org/node/314178"
  },
  "require": {
    "drupal/core-composer-scaffold": "^8.9",
    "drupal/core-recommended": "^8.9",
  },
  "require-dev": {
    "drupal/core-dev": "^8.9",
    "composer/installers": "^1.9",
    "cweagans/composer-patches": "^1.6.5",
    "davidrjonas/composer-lock-diff": "^1.5",
    "szeidler/composer-patches-cli": "^1.0",
    "oomphinc/composer-installers-extender": "^2.0",
    "joachim-n/composer-manifest": "^1.1",
    "drush/drush": "^10.0"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform": {
      "php": "7.3.24"
    }
  },
  "scripts": {
    "pre-update-cmd": [],
    "post-update-cmd": []
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      },
      "file-mapping": {
        ".gitignore": {
          "append": "assets/.gitignore.append.txt",
          "force-append": true
        },
        "[web-root]/robots.txt": {
          "append": "assets/web/robots.txt.append.txt"
        },
        "[web-root]/.htaccess": {
          "append": "assets/web/.htaccess.append.txt"
        }
      }
    },
    "composer-exit-on-patch-failure": true,
    "enable-patching": true,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "patches": {},
    "installer-types": [
      "npm-asset",
      "bower-asset"
    ],
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "web/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ],
      "web/libraries/{$name}": [
        "type:npm-asset",
        "type:bower-asset",
        "type:drupal-library"
      ],
      "drush/Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "web/libraries/ckeditor/plugins/{$name}": [
        "vendor:ckeditor-plugin"
      ]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "kenwheeler/slick",
        "version": "dev-master-patched",
        "type": "drupal-library",
        "_readme": "!! PATCHED !! We're using the patch from here https://github.com/kenwheeler/slick/pull/3674/commits/2a68c0bf58026d66838f9656d93527046646b4fb Without this patch Slick removes all events from subelements, e.g. photoswipe images within Slick.",
        "dist": {
          "url": "https://github.com/webksde/slick/archive/patch-1.zip",
          "type": "zip"
        }
      }
    }
  ]
}
